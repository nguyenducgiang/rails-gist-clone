<!DOCTYPE html>
<html>
<head>
  <title>RailsPastebin</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>
	<div class="container">
		<div class="row"><%= render 'shared/header' %></div>
		<div class="row"><%= yield %></div>
		</br>
		<div class="row"><%= render 'shared/footer' %></div>
	</div>
</body>
</html>
